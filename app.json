{
  "expo": {
    "name": "aonde-tem",
    "slug": "aonde-tem",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F15F7E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Aondetem.app"
    },
    "android": {
      "package": "Aondetem.app"
    },
    "androidStatusBar": {
      "backgroundColor": "#F15F7E"
    }
  }
}
